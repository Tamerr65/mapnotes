<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Base.Theme.MapNotes" parent="Theme.Material3.Light.NoActionBar">

        <item name="colorPrimary">@color/color30</item>
        <item name="colorPrimaryVariant">@color/color30</item>
        <item name="colorOnPrimary">@color/white</item>

        <item name="colorSecondary">@color/color10</item>
        <item name="colorSecondaryVariant">@color/color10</item>
        <item name="colorOnSecondary">@color/black</item>

    </style>

    <style name="Theme.MapNotes" parent="Base.Theme.MapNotes">

        <item name="android:statusBarColor">@color/color60</item>
        <item name="android:windowLightStatusBar">true</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:navigationBarColor">@color/color30</item>
        <item name="android:windowLightNavigationBar">true</item>

    </style>

    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/white</item>
    </style>

</resources>